<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.justbon.oms</groupId>
  <artifactId>fink-oms</artifactId>
  <name>fink-oms</name>
  <version>1.0-SNAPSHOT</version>
  <url>http://www.example.com</url>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.4.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <filters>
            <filter>
              <artifact>*:*</artifact>
              <excludes>
                <exclude>META-INF/*.SF</exclude>
                <exclude>META-INF/*.DSA</exclude>
                <exclude>META-INF/*.RSA</exclude>
              </excludes>
            </filter>
          </filters>
          <transformers>
            <transformer>
              <mainClass>com.justbon.oms.flink.App.FlinkMonitor</mainClass>
            </transformer>
            <transformer>
              <resource>META-INF/spring.handlers</resource>
            </transformer>
            <transformer>
              <resource>META-INF/spring.schemas</resource>
            </transformer>
          </transformers>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.apache.flink</groupId>
      <artifactId>flink-table</artifactId>
      <version>1.9.0</version>
      <type>pom</type>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <properties>
    <flink.version>1.9.0</flink.version>
    <httpclient.version>4.4</httpclient.version>
    <hbase-client.version>1.2.3</hbase-client.version>
    <slf4j.version>1.7.21</slf4j.version>
    <httpcore.version>4.4</httpcore.version>
    <fast.json.version>1.2.47</fast.json.version>
    <google-collections.version>1.0</google-collections.version>
    <maven.compiler.target>1.8</maven.compiler.target>
    <lombok.verion>1.18.2</lombok.verion>
    <org.slf4j-version>1.7.12</org.slf4j-version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <logback.version>1.1.7</logback.version>
    <hadoop.version>2.7.1</hadoop.version>
    <spring-data-hadoop.version>2.4.0.RELEASE</spring-data-hadoop.version>
    <alihbase-client.version>1.1.3</alihbase-client.version>
    <maven.compiler.source>1.8</maven.compiler.source>
  </properties>
</project>

